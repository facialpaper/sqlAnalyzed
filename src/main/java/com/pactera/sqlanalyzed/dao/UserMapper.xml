<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.pactera.sqlanalyzed.dao.UserMapper">

    <select id="queryById" parameterType="java.lang.String" resultType="com.pactera.sqlanalyzed.entity.User">

        select id,user_name as name from t_system_users where id=#{id}

    </select>

</mapper>